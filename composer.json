{
	"name": "adriantamasdevel/voting-competition-api",
	"description": "PHP API for Image Competitions",
	"keywords": [
		"silex", "rest", "api"
	],
	"homepage": "https://github.com/adriantamasdevel/voting-competition-api",
	"license": "proprietary",
	"authors": [{
		"name": "Adrian Tamas",
		"email": "adrian.tamas84@gmail.com"
	}],
	"repositories": [
		{
			"type": "composer",
			"url": ""
		}
	],
	"require": {
		"php": ">=5.4.0",
		"anchovy/curl-bundle": "~1.0",
		"basster/silex-simple-swagger-provider": "~1.0.2",
		"doctrine/dbal": "~2.5.4",
		"egulias/email-validator": "^2.0",
		"igorw/config-service-provider": "~1.2",
		"imagine/imagine": "~0.5.0",
		"knplabs/console-service-provider": "^1.1",
		"kphoen/negotiation-service-provider": "~1.0",
		"marcojanssen/silex-routing-service-provider" : "~1.5",
		"monolog/monolog": "~1.6",
		"nesbot/Carbon": "~1.6",
		"neutron/silex-imagine-provider": "0.1.*",
		"ramsey/uuid": "3.4.1",
		"silex/silex": "~1.3",
		"zircote/swagger-php" : "~2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.1.4",
		"mockery/mockery": "~0.9.1",
		"knplabs/gaufrette": "*",
		"amphp/artax": "v2.0.4",
		"behat/mink": "*",
		"behat/mink-goutte-driver": "*",
		"behat/mink-extension": "*",
		"behat/mink-selenium2-driver": "*",
		"beberlei/assert": "^2.5",
		"justinrainbow/json-schema": "^2.0"
	},
	"autoload": {
		"psr-0": {
			"App": "src/"
		},
		"files": ["src/functions.php"]
	},
	"autoload-dev": {
		"psr-0": {
			"AppTest": "test/"
		},
		"files": ["test/testFunctions.php", "test/FeatureContext.php"]
	},
	"config": {
		"secure-http": false
	}
}
